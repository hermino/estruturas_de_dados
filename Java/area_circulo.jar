import java.io.IOException;
import java.util.Scanner;

public class Main {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        double pi = 3.14159, r, area;

        r = input.nextDouble();

        area = pi * (r*r);

        System.out.printf("A=%.4f\n", area);

    }
}

